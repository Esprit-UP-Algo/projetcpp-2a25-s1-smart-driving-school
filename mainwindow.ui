  <property name="styleSheet">
   <string notr="true">/* === Fenêtre principale === */
QMainWindow, QDialog {
    background-color: #f5f7f4;
}

/* === Boutons latéraux === */
QPushButton {
    background-color: #3c7a6d;
    color: white;
    border-radius: 10px;
    font-weight: bold;
    font-size: 14px;
    padding: 6px;
}

QPushButton:hover {
    background-color: #2f665a;
}

QPushButton:pressed {
    background-color: #28584e;
}

/* === GroupBox === */
QGroupBox {
    background-color: #e8f0ed;
    border: 2px solid #3c7a6d;
    border-radius: 10px;
    margin-top: 10px;
    font-weight: bold;
    padding: 10px;
}

/* === Labels === */
QLabel {
    color: #1f4d42;
    font-weight: bold;
}

/* === QLineEdit, QComboBox === */
QLineEdit, QComboBox {
    border: 1px solid #3c7a6d;
    border-radius: 6px;
    padding: 4px 6px;
    background-color: white;
    color: #1f4d42;
}

/* === Table === */
QTableWidget {
    background-color: white;
    gridline-color: #3c7a6d;
    border: 1px solid #3c7a6d;
    border-radius: 8px;
}

QHeaderView::section {
    background-color: #3c7a6d;
    color: white;
    font-weight: bold;
    padding: 5px;
    border: none;
}

/* === Onglets === */
QTabWidget::pane {
    border: 2px solid #3c7a6d;
    border-radius: 10px;
}

QTabBar::tab {
    background: #dbe7e3;
    color: #1f4d42;
    padding: 6px 12px;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
}

QTabBar::tab:selected {
    background: #3c7a6d;
    color: white;
}
</string>
