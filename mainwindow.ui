         <property name="styleSheet">
          <string notr="true">/* === Palette et base === */
QMainWindow, QWidget {
    background-color: #F3F1E7;
    font-family: &quot;Poppins&quot;, &quot;Segoe UI&quot;, sans-serif;
    color: #2E3A59;
}

/* Side menu */
#sideMenu {
    background-color: #1E4D2B;
}
#sideMenu QPushButton {
    background: transparent;
    color: #F3F1E7;
    border: none;
    padding: 12px;
    text-align: left;
    font-weight: 600;
}
#sideMenu QPushButton:hover { background-color: #2E3A59; border-radius: 6px; }
#sideMenu QPushButton:checked { background-color: #F3F1E7; color: #1E4D2B; border-radius: 6px; }

/* Title */
QLabel#titleLabel {
    font: bold 20pt &quot;Poppins&quot;;
    color: #1E4D2B;
    margin-bottom: 8px;
}

/* GroupBoxes (base) */
QGroupBox {
    background-color: #FFFFFF;
    border: 2px solid #E2DCC5;
    border-radius: 8px;
    margin-top: 10px;
    padding: 12px;
}
QGroupBox::title { color: #1E4D2B; font-weight: bold; }

/* Specific colors per module */
QGroupBox#groupBox_add {
    border-color: #CFE8DC;
    background: qlineargradient(x1:0,y1:0,x2:1,y2:1, stop:0 #E9F7F0, stop:1 #DFF3E8);
}
QGroupBox#groupBox_search {
    border-color: #D6EAF8;
    background: qlineargradient(x1:0,y1:0,x2:1,y2:1, stop:0 #EEF7FB, stop:1 #E3F2FD);
}
QGroupBox#groupBox_modify {
    border-color: #FFE7CD;
    background: qlineargradient(x1:0,y1:0,x2:1,y2:1, stop:0 #FFF6EB, stop:1 #FFF0DB);
}
QGroupBox#groupBox_list {
    border-color: #CFE8DC;
    background: white;
}

/* Form fields */
QLineEdit, QDateEdit, QComboBox {
    background: white;
    border: 1px solid #C5C5C5;
    border-radius: 6px;
    padding: 6px;
}
QLineEdit:focus, QDateEdit:focus, QComboBox:focus {
    border: 2px solid #1E4D2B;
}

/* Buttons */
QPushButton {
    background-color: #2E3A59;
    color: #F3F1E7;
    border-radius: 8px;
    padding: 8px 14px;
    font-weight: 600;
}
QPushButton:hover { background-color: #1E4D2B; }

QPushButton#addButton { background-color: #1E4D2B; }
QPushButton#addButton:hover { background-color: #2E3A59; }

QPushButton#btnSearch { background-color: #2E3A59; }
QPushButton#modifyButton { background-color: #F57C00; }
QPushButton#deleteButton { background-color: #C62828; }
QPushButton#exportButton { background-color: #166534; }
QPushButton#quitButton { background-color: #455A64; }

/* Table */
QTableWidget {
    background: white;
    border: 1px solid #D7E6DE;
    border-radius: 6px;
    gridline-color: #EAEAEA;
}
QHeaderView::section {
    background-color: #1E4D2B;
    color: white;
    padding: 6px;
    font-weight: bold;
}
</string>
